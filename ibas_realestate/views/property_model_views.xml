<odoo>
<data>
    <record id="ibas_realestate_property_model_form" model="ir.ui.view">
        <field name="name">ibas_realestate_property_model_form</field>
        <field name="model">ibas_realestate.propertymodel</field>
        <field name="arch" type="xml">
            <form string="Property Model">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_ibas_realestate_property_model" model="ir.actions.act_window">
        <field name="name">Property Model</field>
        <field name="res_model">ibas_realestate.propertymodel</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
    </record>

    <menuitem
        id="ibas_realestate_property_model_menu"
        name="Model"
        action="action_ibas_realestate_property_model"
        groups="ibas_realestate.group_ibas_realestate_administrator"
        parent="ibas_re_menu"
        sequence="30"/>
</data>
</odoo>