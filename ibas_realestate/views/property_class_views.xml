<odoo>
<data>

    <record id="ibas_realestate_property_class_form" model="ir.ui.view">
        <field name="name">ibas_realestate_property_class_form</field>
        <field name="model">ibas_realestate.property_class</field>
        <field name="arch" type="xml">
            <form string="Property Type">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_ibas_realestate_property_class" model="ir.actions.act_window">
        <field name="name">Property Type</field>
        <field name="res_model">ibas_realestate.property_class</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
    </record>

    <menuitem
        id="ibas_realestate_property_class_menu"
        name="Type"
        action="action_ibas_realestate_property_class"
        parent="ibas_re_menu"
        sequence="20"/>
</data>
</odoo>